{
  "version": 1,
  "hooks": {
    "beforeSubmitPrompt": [
      {
        "command": ".cursor/hooks/uilint-session-start.sh"
      }
    ],
    "afterFileEdit": [
      {
        "command": ".cursor/hooks/uilint-track.sh"
      }
    ],
    "stop": [
      {
        "command": ".cursor/hooks/uilint-session-end.sh"
      }
    ]
  }
}