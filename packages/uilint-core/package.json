{
  "name": "uilint-core",
  "version": "0.1.0",
  "description": "Core library for UILint - AI-powered UI consistency checking",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "dependencies": {
    "jsdom": "^25.0.1"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "ui",
    "lint",
    "consistency",
    "design-system",
    "ai",
    "llm",
    "ollama"
  ],
  "license": "MIT"
}

