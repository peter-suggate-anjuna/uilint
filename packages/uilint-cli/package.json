{
  "name": "uilint-cli",
  "version": "0.1.0",
  "description": "CLI for UILint - AI-powered UI consistency checking",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "uilint": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.1.1",
    "uilint-core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "cli",
    "ui",
    "lint",
    "consistency",
    "design-system",
    "ai"
  ],
  "license": "MIT"
}
